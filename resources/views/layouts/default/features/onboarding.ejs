<script>
  async function submit() {
    const nickname = document.getElementById("nickname").value;
    const language = document.getElementById("language").value ?? "en"
    const first = document.getElementById("first").value;
    const last = document.getElementById("last").value ?? ""

    try {
      const response = await fetch(`/api/onboarding/submit?nickname=${nickname}&language=${language}&first=${first}&last=${last}`, {
        method: "GET",
        headers: {
          "Content-Type": "application/json"
        },
      });
      const xD = await response.json()
      if (xD.success == true) {
        window.location.href = "/onboarding/finalize";
      } else {
        alert(xD.message);
      }
    } catch (error) {
      console.error("An error occurred while completing onboarding:", error);
      alert("An error occurred while completing onboarding. Please try again later.");
    }
  };
</script>

