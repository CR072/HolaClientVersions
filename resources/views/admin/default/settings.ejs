                                      <div class="bg-zinc-800 w-full rounded-lg text-center p-4 items-center">
                                        <label class="block text-gray-300">SEO Image</label>
                                        <img id="seoimgr" class="rounded-lg" src="<%= settings.seo.image %>">
                                      </div>
                                    </div>
                                  </div>
                                </div>
                              </div>
                            </div>
                          </div>
                          <div class="hidden rounded-lg mb-4 gap-4" id="auth" role="tabpanel">
                            <div class="flex bg-zinc-900 text-zinc-300 lg:w-1/2 rounded-lg flex-wrap p-4 content-start">
                              <h1 class="text-2xl text-zinc-100"><i class="fa-brands fa-discord mr-4"></i>Discord Oauth2</h1>
                              <hr class="w-full border-b border-zinc-700 my-2">
                              <div class="w-full mt-4 grid grid-cols-1 gap-4">
                                <div class="w-full pr-2">
                                  <label class="block text-gray-300">Client ID</label>
                                  <input class="w-full bg-zinc-800 rounded-lg p-2" id="dccid" value="<%= settings.authentication.discord.id %>" placeholder="06090609 just tryna be professional tho">
                                </div>
                                <div class="w-full pr-2">
                                  <label class="block text-gray-300">Client Secret</label>
                                  <input class="w-full bg-zinc-800 rounded-lg p-2" id="dccs" value="<%= settings.authentication.discord.secret %>">
                                </div>
                                <div class="w-full pr-2">
                                  <label class="block text-gray-300">Redirect URI (Use this as your Oauth2 app's Redirect URI)</label>
                                  <input class="w-full bg-zinc-800 rounded-lg p-2" value="<%= process.env.APP_URL + settings.authentication.discord.callbackpath %>" disabled>
                                </div>
                                <div class="flex">
                                  <div class="w-full pr-2">
                                    <label class="block text-gray-300">Enable driver (Disabling is not possible)</label>
                                    <div class="items-center py-1.5 flex">
                                      <label class="relative inline-flex items-center cursor-pointer">
                                        <input type="checkbox" class="sr-only peer" disabled checked>
                                        <div class="w-11 h-6 bg-gray-200 peer-focus:outline-none peer-focus:ring-4 peer-focus:ring-blue-300 dark:peer-focus:ring-blue-800 rounded-full peer dark:bg-gray-700 peer-checked:after:translate-x-full peer-checked:after:border-white after:content-[''] after:absolute after:top-[2px] after:left-[2px] after:bg-white after:border-gray-300 after:border after:rounded-full after:h-5 after:w-5 after:transition-all dark:border-gray-600 peer-checked:bg-blue-600"></div>
                                        <span class="ml-3 text-sm font-medium text-gray-900 dark:text-gray-300">Enable Discord authentication</span>
                                      </label>
                                    </div>
                                  </div>
                                  <div class="w-full pr-2">
                                    <label class="block text-gray-300">Allow signups</label>
                                    <div class="items-center py-1.5 flex">
                                      <label class="relative inline-flex items-center cursor-pointer" checked disabled>
                                        <input type="checkbox" value="" class="sr-only peer">
                                        <div class="w-11 h-6 bg-gray-200 peer-focus:outline-none peer-focus:ring-4 peer-focus:ring-blue-300 dark:peer-focus:ring-blue-800 rounded-full peer dark:bg-gray-700 peer-checked:after:translate-x-full peer-checked:after:border-white after:content-[''] after:absolute after:top-[2px] after:left-[2px] after:bg-white after:border-gray-300 after:border after:rounded-full after:h-5 after:w-5 after:transition-all dark:border-gray-600 peer-checked:bg-blue-600"></div>
                                        <span class="ml-3 text-sm font-medium text-gray-900 dark:text-gray-300">Enable Discord registrations</span>
                                      </label>
                                    </div>
                                  </div>
                                </div>
                                <div class="flex">
                                  <div class="w-full pr-2">
                                    <label class="block text-gray-300">Cookie Alt Check</label>
                                    <div class="items-center py-1.5 flex">
                                      <label class="relative inline-flex items-center cursor-pointer">
                                        <input type="checkbox" id="cookiealt" class="sr-only peer" <% if (settings.authentication.discord.alt.cookies) { %> checked <% } %>/>
                                        <div class="w-11 h-6 bg-gray-200 peer-focus:outline-none peer-focus:ring-4 peer-focus:ring-blue-300 dark:peer-focus:ring-blue-800 rounded-full peer dark:bg-gray-700 peer-checked:after:translate-x-full peer-checked:after:border-white after:content-[''] after:absolute after:top-[2px] after:left-[2px] after:bg-white after:border-gray-300 after:border after:rounded-full after:h-5 after:w-5 after:transition-all dark:border-gray-600 peer-checked:bg-blue-600"></div>
                                        <span class="ml-3 text-sm font-medium text-gray-900 dark:text-gray-300">Block alts by checking cookies.</span>
                                      </label>
                                    </div>
                                  </div>
                                  <div class="w-full pr-2">
                                    <label class="block text-gray-300">IPv6 Alt Check</label>
                                    <div class="items-center py-1.5 flex">
                                      <label class="relative inline-flex items-center cursor-pointer">
                                        <input type="checkbox" id="ipalt" class="sr-only peer" <% if (settings.authentication.discord.alt.ip) { %> checked <% } %>>
                                        <div class="w-11 h-6 bg-gray-200 peer-focus:outline-none peer-focus:ring-4 peer-focus:ring-blue-300 dark:peer-focus:ring-blue-800 rounded-full peer dark:bg-gray-700 peer-checked:after:translate-x-full peer-checked:after:border-white after:content-[''] after:absolute after:top-[2px] after:left-[2px] after:bg-white after:border-gray-300 after:border after:rounded-full after:h-5 after:w-5 after:transition-all dark:border-gray-600 peer-checked:bg-blue-600"></div>
                                        <span class="ml-3 text-sm font-medium text-gray-900 dark:text-gray-300">Block alts by checking IPv6.</span>
                                      </label>
                                    </div>
                                  </div>
                                </div>
                              </div>
                            </div>
                          </div>
                          <div class="hidden rounded-lg mb-4" id="links" role="tabpanel">
                            <div class="flex bg-zinc-900 text-zinc-300 lg:w-1/2 rounded-lg flex-wrap p-4 content-start">
                                <h1 class="text-2xl text-zinc-100">Add social links</h1>
                                <hr class="w-full border-b border-zinc-700 my-2">
                                Unavailable in this theme!
                            </div>
                          </div>
                          <div class="hidden rounded-lg mb-4" id="landing" role="tabpanel">
                            <div class="flex bg-zinc-900 text-zinc-300 rounded-lg flex-wrap p-4 content-start">
                              <h1 class="text-2xl text-zinc-100">Landing pages</h1>
                              <hr class="w-full border-b border-zinc-700 my-2">
                              <div class="mt-4 grid grid-cols-1 lg:grid-cols-3 overflow-auto">
                                <div class="w-full text-center">
                                  <img class="rounded-lg border-4 border-blue-500" src="https://media.discordapp.net/attachments/1108054221456146534/1190284354996416532/image.png">
                                  <span class="mt-2 text-lg">Default</span>
                                </div>
                              </div>
                            </div>
                            <div class="flex bg-zinc-900 text-zinc-300 rounded-lg flex-wrap p-4 content-start mt-4">
                              <h1 class="text-2xl text-zinc-100">Default page options</h1>
                              <hr class="w-full border-b border-zinc-700 my-2">
                              <p>No settings found for the theme: default</p>
                            </div>
                          </div>
                          <div class="hidden rounded-lg mb-4" id="smtp" role="tabpanel">
                            <div class="bg-zinc-900 rounded-lg text-white mt-4 p-4">
                                <span class="text-2xl">
                                    SMTP has been temporarily removed. Sorry for the inconvenience.
                                </span>
                            </div>
                            <div class="flex bg-zinc-900 text-zinc-300 rounded-lg flex-wrap p-4 content-start mt-4">
                              <h1 class="text-2xl text-zinc-100">Configure SMTP</h1>
                              <hr class="w-full border-b border-zinc-700 my-2">
                              <div class="mt-4 w-full grid grid-cols-1 lg:grid-cols-3 gap-12">
                                <div class="w-full">
                                  <label class="mb-2 inline-block w-full cursor-pointer text-sm">SMTP Status</label>
                                  <select class="w-full outline-0 text-white border-2 border-zinc-800 bg-zinc-800 p-2 text-base rounded-lg">
                                  <option>Disabled</option>
                                  <option>Enabled</option>
                                  </select>
                                </div>
                                <div class="w-full col-span-2">
                                  <label class="mb-2 inline-block w-full cursor-pointer text-sm">Host</label>
                                  <input class="w-full outline-0 text-white border-2 border-zinc-800 bg-zinc-800 p-2 text-base rounded-lg"
                                  value="HolaClient | Landing">
                                </div>
                                <div class="w-full">
                                  <label class="mb-2 inline-block w-full cursor-pointer text-sm">Username</label>
                                  <input class="w-full outline-0 text-white border-2 border-zinc-800 bg-zinc-800 p-2 text-base rounded-lg"
                                  value="HolaClient | Landing">
                                </div>
                                <div class="w-full">
                                  <label class="mb-2 inline-block w-full cursor-pointer text-sm">Password</label>
                                  <input class="w-full outline-0 text-white border-2 border-zinc-800 bg-zinc-800 p-2 text-base rounded-lg"
                                  value="HolaClient | Landing">
                                </div>
                                <div class="w-full">
                                  <label class="mb-2 inline-block w-full cursor-pointer text-sm">Port</label>
                                  <input class="w-full outline-0 text-white border-2 border-zinc-800 bg-zinc-800 p-2 text-base rounded-lg"
                                  value="HolaClient | Landing">
                                </div>
                              </div>
                              <button class="w-full mt-4 outline-0 text-white bg-zinc-800 hover:bg-zinc-800/40 p-2 text-base rounded-lg">
                                Apply changes
                              </button>
                            </div>
                            <div class="relative flex bg-zinc-900 text-zinc-300 rounded-lg mt-4">
                              <div class="p-4 flex flex-wrap content-start">
                                <h1 class="text-2xl text-zinc-100">Create SMTP server</h1>
                              <hr class="w-full border-b border-zinc-700 my-2">
                              <div class="mt-4 w-full grid grid-cols-1 lg:grid-cols-3 gap-12">
                                <div class="w-full">
                                  <label class="mb-2 inline-block w-full cursor-pointer text-sm">Type</label>
                                  <select class="w-full outline-0 text-white border-2 border-zinc-800 bg-zinc-800 p-2 text-base rounded-lg">
                                  <option>Send & Receive mails</option>
                                  <option>Send mails</option>
                                  <option>Receive mails</option>
                                  </select>
                                </div>
                                <div class="w-full">
                                  <label class="mb-2 inline-block w-full cursor-pointer text-sm">Host</label>
                                  <input class="w-full outline-0 text-white border-2 border-zinc-800 bg-zinc-800 p-2 text-base rounded-lg"
                                  value="smtp.holaclient.tech">
                                </div>
                                <div class="w-full">
                                  <label class="mb-2 inline-block w-full cursor-pointer text-sm">Encryption</label>
                                  <select class="w-full outline-0 text-white border-2 border-zinc-800 bg-zinc-800 p-2 text-base rounded-lg">
                                    <option>SSL</option>
                                    <option>null</option>
                                    </select>
                                </div>
                                <div class="w-full">
                                  <label class="mb-2 inline-block w-full cursor-pointer text-sm">Username</label>
                                  <input class="w-full outline-0 text-white border-2 border-zinc-800 bg-zinc-800 p-2 text-base rounded-lg"
                                  value="test">
                                </div>
                                <div class="w-full">
                                  <label class="mb-2 inline-block w-full cursor-pointer text-sm">Password</label>
                                  <input class="w-full outline-0 text-white border-2 border-zinc-800 bg-zinc-800 p-2 text-base rounded-lg"
                                  value="test">
                                </div>
                                <div class="w-full">
                                  <label class="mb-2 inline-block w-full cursor-pointer text-sm">Port</label>
                                  <input class="w-full outline-0 text-white border-2 border-zinc-800 bg-zinc-800 p-2 text-base rounded-lg"
                                  value="25">
                                </div>
                              </div>
                              <button class="w-full mt-4 outline-0 text-white bg-zinc-800 hover:bg-zinc-800/40 p-2 text-base rounded-lg">
                                Initiate creating
                              </button>
                              </div>
                              <div class="absolute text-2xl text-white backdrop-blur-[2px] rounded-lg w-full h-full flex items-center justify-center">
                                This thing will be fixed in the future update.<br>
                                I wasted too much time on this $h!t
                              </div>
                            </div>
                          </div>
                          <div class="hidden rounded-lg mb-4" id="href" role="tabpanel">
                            <div class="flex bg-zinc-900 text-zinc-300 rounded-lg flex-wrap p-4 content-start mt-4">
                              <h1 class="text-2xl text-zinc-100">Page redirects</h1>
                              <hr class="w-full border-b border-zinc-700 my-2">
                              <div class="mt-4 w-full">
                                <table class="w-full table-fixed bg-zinc-800 rounded-lg">
                                  <thead class="text-white justify-between border-b border-zinc-700">
                                    <tr>
                                      <th class="p-4 text-left flex-grow md:flex-col">
                                          <div class="cursor-pointer flex items-center">
                                            URL
                                          </div>
                                        </th>
                                      <th class="p-4 text-left">
                                        <div class="cursor-pointer flex items-center">
                                          Redirect
                                        </div>
                                      </th>
                                      <th class="p-4 text-right">
                                        <div class="flex items-center">
                                          Actions
                                        </div>
                                      </th>
                                    </tr>
                                  </thead>
                                  <tbody id="tbody">
                                    
                                  </tbody>
                                </table>
                                <hr class="w-full border-b border-zinc-700 my-2">
                                <div class="flex mt-4 gap-4">
                                  <div class="w-1/5">
                                    <span>Type</span>
                                    <select class="w-full outline-0 text-white border-2 border-zinc-800 bg-zinc-800 p-2 text-base rounded-lg">
                                    <option>301 - Permanent</option>
                                    <option>302 - Temporary</option>
                                    </select>
                                  </div>
                                  <div class="w-2/5">
                                    <span>URL</span>
                                    <input id="e" class="w-full outline-0 text-white border-2 border-zinc-800 bg-zinc-800 p-2 text-base rounded-lg"
                                  placeholder="discord">
                                  </div>
                                  <div class="w-2/5">
                                    <span>Redirect</span>
                                    <input id="r" class="w-full outline-0 text-white border-2 border-zinc-800 bg-zinc-800 p-2 text-base rounded-lg"
                                  placeholder="https://discord.gg/CvqRH9TrYK">
                                  </div>
                                </div>
                              </div>
                              <button onclick="red()" class="w-full mt-4 outline-0 text-white bg-zinc-800 hover:bg-zinc-800/40 p-2 text-base rounded-lg">
                                Add redirect | Note: You've to restart the dashboard to active redirect links!
                              </button>
                              <script>
                                async function red() {
                                  const e = document.getElementById("e").value;
                                  const r = document.getElementById("r").value;
                                  const response = await fetch(`/api/core/redirects/add?e=${e}&r=${r}`);
                                  const data = await response.json();
                                  if (data.success == true) {
                                    toastr.success("Success");
                                    upd();
                                  }
                                }

                                async function deleteRedirect(e) {
                                    const response = await fetch(`/api/core/redirects/remove?e=${e}`);
                                    const data = await response.json();
                                     if (data.success == true) {
                                       toastr.success("Success");
                                       upd();
                                     }
                                }
                              
                                async function upd() {
                                  let response = await fetch('/api/core/redirects');
                                  let data = await response.json();
                              
                                    document.getElementById("tbody").innerHTML = data.map(i => `
                                      <tr>
                                        <td class="p-4 transition-all duration-300 ease-in-out text-left">
                                          <a href="">
                                            <div class="text-zinc-300 text-lg">/${i.endpoint}</div>
                                          </a>
                                        </td>
                                        <td class="p-4 transition-all duration-300 ease-in-out text-left">
                                          <a href="">
                                            <div class="text-zinc-300 text-lg">${i.redirect}</div>
                                          </a>
                                        </td>
                                        <td class="p-4 transition-all duration-300 ease-in-out text-left">
                                          <div class="flex items-center text-zinc-300 gap-2">
