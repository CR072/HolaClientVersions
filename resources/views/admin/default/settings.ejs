                                  <input id="seokey" class="w-full bg-zinc-800 outline-none rounded-lg p-2" value="<%= settings.seo.keywords %>">
                                </div>
                              </div>
                              <div class="flex w-full mt-2">
                                <div class="grid grid-cols-1 lg:grid-cols-3 gap-4">
