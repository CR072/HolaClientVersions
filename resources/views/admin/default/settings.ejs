                            <li class="w-full" role="presentation">
                              <button class="inline-block w-full p-4 bg-white hover:text-gray-700 hover:bg-gray-50 dark:hover:text-white dark:bg-zinc-800/40 dark:hover:bg-zinc-800" data-tabs-target="#smtp" type="button" role="tab" aria-controls="smtp" aria-selected="false">SMTP</button>
                            </li>
                            <li class="w-full" role="presentation">
                              <button class="inline-block w-full p-4 bg-white hover:text-gray-700 hover:bg-gray-50 rounded-r-xl dark:hover:text-white dark:bg-zinc-800/40 dark:hover:bg-zinc-800" data-tabs-target="#href" type="button" role="tab" aria-controls="href" aria-selected="false">Redirects</button>
                            </li>
                          </ul>
                        </div>
                        <div id="userBox">
                          <div class="hidden rounded-xl mb-4" id="general" role="tabpanel">
                            <div class="flex bg-zinc-900 rounded-lg text-zinc-300 flex-wrap p-4 content-start">
                              <h1 class="text-2xl text-zinc-100">General Settings</h1>
                              <hr class="w-full border-b border-zinc-700 my-2">
                              <div class="w-full mt-4 grid grid-cols-2 lg:grid-cols-3 gap-4">
                                <div class="w-full pr-2">
                                  <label class="block text-gray-300">Application name</label>
                                  <input id="appname" class="w-full bg-zinc-800 outline-none rounded-lg p-2" value="<%= settings.name %>">
                                </div>
                                <div class="w-full pr-2">
                                  <label class="block text-gray-300">Application url</label>
                                  <input class="w-full bg-zinc-800 rounded-lg p-2" value="<%= process.env.APP_URL %>" disabled>
                                </div>
                                <div class="w-full pr-2">
                                  <label class="block text-gray-300">Application type</label>
                                  <select  class="w-full bg-zinc-800 rounded-lg p-2" disabled>
                                    <option value="prod"><%= process.env.APP_ENV %></option>
                                  </select>
                                </div>
                                <div class="w-full pr-2">
                                  <label class="block text-gray-300">Application logo</label>
                                  <input id="applogo" class="w-full bg-zinc-800 outline-none rounded-lg p-2" value="<%= settings.logo.url %>">
                                </div>
                                <div class="w-full pr-2">
                                  <label class="block text-gray-300">Application currency</label>
                                  <input class="w-full bg-zinc-800 outline-none rounded-lg p-2" value="<%= cryptomus.currency.default %>">
                                </div>
                                <div class="w-full pr-2">
                                  <label class="block text-gray-300">Rotate logo?</label>
                                  <select id="applogor" class="w-full outline-none bg-zinc-800 rounded-lg p-2">
                                    <option value="true">Yes, please</option>
                                    <option value="false">Hell nah</option>
                                  </select>
                                </div>
                              </div>
                            </div>
                            <div class="flex bg-zinc-900 text-zinc-300 rounded-lg flex-wrap p-4 content-start mt-4">
